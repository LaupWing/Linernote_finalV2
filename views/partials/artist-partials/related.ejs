<ul class="related-list">
    <% related.forEach((artist)=> { %>
      <li class="related-item"><a href="/artist/<%= artist.spotifyId %>&<%= artist.id %>">
        <% if(artist.img){ %>
            <div class="related-item-img <%= artist.border %>">
                <img src="<%= artist.img %>">
            </div>
        <% } else{ %>
            <div class="related-item-img <%= artist.border %>">
                <img src="/img/placeholder.png">
            </div>	
        <% } %>
        <h4 class="related-item-name"><%= artist.artist %></h4></a>
    </li>
    <% }) %>
  </ul>
  